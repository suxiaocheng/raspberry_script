[Unit]
Description=Record temperature Service
After=syslog.target network.target remote-fs.target nss-lookup.target

[Service]    
Type=forking   
ExecStart=/home/pi/temperature/get_temp_start.sh
ExecStop=/home/pi/temperature/get_temp_stop.sh
PrivateTmp=true   
   
[Install]      
WantedBy=multi-user.target
